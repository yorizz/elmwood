<div class="modal-dialog modal-dialog-centered">
    <% if(locals.t_client) { %> <% const cl = t_client[0]; %>
    <form action="/client/<%= cl.c_ID %>/addfile"  id="add-client-file"  method="post" enctype="multipart/form-data" >
        <input type="hidden" name="nepvraag" id="nepvraag">

        <input type="hidden" name="clientID" id="clientID" value="<%= cl.c_ID %>">

        <label for="client_file">file</label>
        <input type="file" name="client_file" id="client_file" class="form-control form-control-file" >

    </form>
    <% } %>
</div>