<% let pathCorrection = (locals.pathCorrection)?locals.pathCorrection:""; %>
<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="robots" content="noindex">
		<meta
			name="viewport"
			content="width=device-width, initial-scale=1.0"
		/>
		<title><%= (locals.title)?title:'Elmwood Admin Dashboard' %></title>
		<link
			rel="shortcut icon"
			href="<%= pathCorrection %>./assets/img/favicon.png"
			type="image/x-icon"
		/>
		<link
			rel="stylesheet"
			href="<%= pathCorrection %>./assets/css/bootstrap-update.css"
		/>
		<link
			rel="stylesheet"
			href="<%= pathCorrection %>./assets/css/bootstrap-update.css.map"
		/>
		<link
			rel="stylesheet"
			href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css"
		/>
		<link
			rel="stylesheet"
			href="<%= pathCorrection %>/assets/css/filter_multi_select.css"
		/>
		<link
			rel="stylesheet"
			href="<%= pathCorrection %>./assets/css/style.css"
		/>

		<script
			type="text/javascript"
			src="<%= pathCorrection %>./assets/js/bootstrap.bundle.min.js"
		></script>
		

		
		
		
	</head>

	<body>
		<div class="container">
			<div id="wrapper">
				

				<div
					id="content-wrapper"
					class="d-flex flex-column"
				>
					

					<!-- Main Content -->
					<div id="content col-md-9">
						<div class="container-fluid">
							<div class="row text-center m404">
								
								<h1>404 | Page not found</h1>
								<p><%= (locals.message) ? message : 'Something went wrong'; %></p>

                                <div class="mb-3 forgotten-password-link">
                                    <a href="/dashboard">back to the dashboard</a>
                                </div>
							
                            </div>
						</div>
					</div>
				</div>
			</div>
			<%- include('../partials/footer.ejs') %>

			<!--  end #wrapper -->
		</div>
			
	</body>
</html>


  